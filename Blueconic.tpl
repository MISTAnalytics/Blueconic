const log = require('logToConsole');
const injectScript = require('injectScript');

const blueconicHost = data.blueconic_host;
const trackingUrl = "https://cdn.blueconic.net/" + blueconicHost + ".js";

const onSuccess = () => {
  log('Blueconic loaded successfully.');
  data.gtmOnSuccess();
};

const onFailure = () => {
  log('Blueconic failed to load.');
  data.gtmOnFailure();
};

injectScript(trackingUrl, data.gtmOnSuccess, data.gtmOnFailure);


